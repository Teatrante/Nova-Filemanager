{
    "name": "wdgt/nova-filemanager",
    "description": "Filemanager tool for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "filemanager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuele",
            "email": "mr.caolila@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "Wdgt\\Filemanager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wdgt\\Filemanager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wdgt\\Filemanager\\FilemanagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
